{
  "compilerOptions": {
	"module": "commonjs", 
	"moduleResolution": "node",
	"allowSyntheticDefaultImports": true,
	"lib": ["es5", "es6", "es7", "es2017", "dom"],
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "lib",
	"jsx": "react",
	"declaration": true,
	"esModuleInterop": true,
	"experimentalDecorators": true,
	"baseUrl": "src"
  }
}